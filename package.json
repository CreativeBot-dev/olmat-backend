{
  "name": "app-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm -- --dataSource=src/database/data-source.ts migration:generate",
    "migration:create": "yarn typeorm -- migration:create",
    "migration:run": "yarn typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "yarn typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "schema:drop": "yarn typeorm -- --dataSource=src/database/data-source.ts schema:drop",
    "seed:create": "hygen seeds create",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "1.8.1",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "10.0.3",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.0.3",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/platform-socket.io": "^10.2.4",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/swagger": "7.0.4",
    "@nestjs/typeorm": "10.0.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^4.1.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dayjs": "^1.11.8",
    "deep-diff": "^1.0.2",
    "express-basic-auth": "^1.2.1",
    "handlebars": "4.7.7",
    "helmet": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.3.5",
    "nodemailer": "6.9.3",
    "passport": "0.6.0",
    "passport-anonymous": "1.0.1",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "sharp": "^0.32.1",
    "swagger-ui-express": "4.6.3",
    "time2fa": "^1.2.2",
    "typeorm": "^0.3.17",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.0.3",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/multer": "1.4.7",
    "@types/node": "20.3.1",
    "@types/passport-anonymous": "1.0.3",
    "@types/passport-jwt": "3.0.8",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "env-cmd": "10.1.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "is-ci": "3.0.1",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.5.3",
    "typescript": "5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
